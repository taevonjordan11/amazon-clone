</small>\n          </>\n        )}\n        decimalScale={2}\n        value={getBasketTotal(basket)}\n        displayType={\"text\"}\n        thousandSerperator={true}\n        prefix={\"$\"}\n      />\n\n      <button>Proceed to Checkout</button>\n    </div>\n  );\n}\n\nexport default Subtotal;\n","/Users/nude.plus/Development/code/amazon-react/amazon-clone/src/StateProvider.js",[],"/Users/nude.plus/Development/code/amazon-react/amazon-clone/src/reducer.js",[],["64","65"],"/Users/nude.plus/Development/code/amazon-react/amazon-clone/src/CheckoutProduct.js",["66","67"],"/Users/nude.plus/Development/code/amazon-react/amazon-clone/src/Login.js",["68"],"import React, { useState } from \"react\";\nimport { Link, useHistory } from \"react-router-dom\";\nimport { auth } from \"./firebase\";\nimport \"./Login.css\";\n\nfunction Login() {\n  const history = useHistory();\n  const [email, setEmail] = useState(\" \");\n  const [password, setPassword] = useState(\" \");\n  const signIn = (e) => {\n    e.preventDefault();\n    auth\n      .signInWithEmailAndPassword(email, password)\n      .then((auth) => {\n        history.push(\"/\");\n      })\n      .catch((error) => alert(error.message));\n  };\n\n  const register = (e) => {\n    e.preventDefault();\n    auth\n      .createUserWithEmailAndPassword(email, password)\n      .then((auth) => {\n        console.log(auth);\n        if (auth) {\n          history.push(\"/\");\n        }\n      })\n      .catch((error) => alert(error.message));\n  };\n\n  return (\n    <div className=\"login\">\n      <Link to=\"/\">\n        <img\n          className=\"login__logo\"\n          src=\"https://purepng.com/public/uploads/large/amazon-logo-s3f.png\"\n        />\n      </Link>\n\n      <div className=\"login__container\">\n        <h1>Sign In</h1>\n        <form>\n          <h5>E-Mail</h5>\n          <input\n            type=\"text\"\n            value={email}\n            onChange={(e) => setEmail(e.target.value)}\n          />\n          <h5>Password</h5>\n\n          <input\n            type=\"password\"\n            value={password}\n            onChange={(e) => setPassword(e.target.value)}\n          />\n\n          <button\n            className=\"login__signInButton\"\n            type=\"submit\"\n            onClick={signIn}\n          >\n            Sign In\n          </button>\n        </form>\n        <button className=\"login__registerButton\" onClick={register}>\n          Create New Account\n        </button>\n      </div>\n    </div>\n  );\n}\n\nexport default Login;\n","/Users/nude.plus/Development/code/amazon-react/amazon-clone/src/firebase.js",[],{"ruleId":"69","severity":1,"message":"70","line":9,"column":22,"nodeType":"71","messageId":"72","endLine":9,"endColumn":30},{"ruleId":"73","replacedBy":"74"},{"ruleId":"75","replacedBy":"76"},{"ruleId":"69","severity":1,"message":"70","line":8,"column":22,"nodeType":"71","messageId":"72","endLine":8,"endColumn":30},{"ruleId":"69","severity":1,"message":"70","line":8,"column":22,"nodeType":"71","messageId":"72","endLine":8,"endColumn":30},{"ruleId":"73","replacedBy":"77"},{"ruleId":"75","replacedBy":"78"},{"ruleId":"69","severity":1,"message":"79","line":6,"column":14,"nodeType":"71","messageId":"72","endLine":6,"endColumn":20},{"ruleId":"80","severity":1,"message":"81","line":18,"column":7,"nodeType":"82","endLine":18,"endColumn":61},{"ruleId":"80","severity":1,"message":"81","line":36,"column":9,"nodeType":"82","endLine":39,"endColumn":11},"no-unused-vars","'dispatch' is assigned a value but never used.","Identifier","unusedVar","no-native-reassign",["83"],"no-negated-in-lhs",["84"],["83"],["84"],"'basket' is assigned a value but never used.","jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","no-global-assign","no-unsafe-negation"]