Sign In\n          </button>\n        </form>\n        <button className=\"login__registerButton\" onClick={register}>\n          Create New Account\n        </button>\n      </div>\n    </div>\n  );\n}\n\nexport default Login;\n","/Users/nude.plus/Development/code/amazon-react/amazon-clone/src/firebase.js",[],{"ruleId":"80","severity":1,"message":"81","line":14,"column":10,"nodeType":"82","messageId":"83","endLine":14,"endColumn":12},{"ruleId":"84","severity":1,"message":"85","line":36,"column":6,"nodeType":"86","endLine":36,"endColumn":8,"suggestions":"87"},{"ruleId":"88","replacedBy":"89"},{"ruleId":"90","replacedBy":"91"},{"ruleId":"92","severity":1,"message":"93","line":10,"column":28,"nodeType":"94","messageId":"95","endLine":10,"endColumn":36},{"ruleId":"88","replacedBy":"96"},{"ruleId":"90","replacedBy":"97"},{"ruleId":"88","replacedBy":"98"},{"ruleId":"90","replacedBy":"99"},{"ruleId":"92","severity":1,"message":"100","line":6,"column":12,"nodeType":"94","messageId":"95","endLine":6,"endColumn":18},{"ruleId":"92","severity":1,"message":"93","line":8,"column":28,"nodeType":"94","messageId":"95","endLine":8,"endColumn":36},{"ruleId":"92","severity":1,"message":"93","line":8,"column":22,"nodeType":"94","messageId":"95","endLine":8,"endColumn":30},{"ruleId":"88","replacedBy":"101"},{"ruleId":"90","replacedBy":"102"},{"ruleId":"92","severity":1,"message":"100","line":6,"column":14,"nodeType":"94","messageId":"95","endLine":6,"endColumn":20},{"ruleId":"103","severity":1,"message":"104","line":18,"column":7,"nodeType":"105","endLine":18,"endColumn":61},{"ruleId":"103","severity":1,"message":"104","line":36,"column":9,"nodeType":"105","endLine":39,"endColumn":11},"no-empty-pattern","Unexpected empty object pattern.","ObjectPattern","unexpected","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'dispatch'. Either include it or remove the dependency array.","ArrayExpression",["106"],"no-native-reassign",["107"],"no-negated-in-lhs",["108"],